{
  "name": "angular-exemplify",
  "version": "2.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hjalmers/angular-exemplify.git"
  },
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "clean": "rimraf exemplify \"index.@(js|js.map|d.ts|metadata.json|ngsummary.json)\"",
    "clean:factory": "rimraf \"exemplify/**/*.ngfactory.ts\"",
    "copy-scss": "copyfiles -f projects/exemplify/src/lib/scss/**/*.scss dist/exemplify/scss/",
    "build-css": "node-sass --source-map true  projects/exemplify/src/lib/scss/prism-exemplify.scss dist/exemplify/css/prism-exemplify.css",
    "autoprefix-css": "postcss -u autoprefixer -r dist/exemplify/css/prism-exemplify.css",
    "minify-css": "cleancss -o dist/exemplify/css/prism-exemplify.min.css dist/exemplify/css/prism-exemplify.css --source-map",
    "compile-css": "npm run build-css && npm run autoprefix-css && npm run minify-css",
    "prebuild": "npm run -s clean",
    "build": "ngc -p src/tsconfig-aot.json && npm run copy:css",
    "build-lib": "ng build exemplify",
    "postbuild": "npm run -s clean:factory",
    "prepublishOnly": "npm run -s build",
    "postpublish": "npm run -s clean",
    "commit": "git-cz"
  },
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "main": "index.js",
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "^6.1.2",
    "@angular/common": "^6.1.1",
    "@angular/compiler": "^6.1.1",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/core": "^6.1.1",
    "@angular/forms": "^6.1.1",
    "@angular/http": "^6.1.1",
    "@angular/platform-browser": "^6.1.1",
    "@angular/platform-browser-dynamic": "^6.1.1",
    "@angular/router": "^6.1.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.116",
    "autoprefixer": "^9.1.0",
    "bootstrap": "^4.1.3",
    "clean-css-cli": "^4.2.1",
    "codelyzer": "~4.2.1",
    "commitizen": "^2.10.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "jasmine-core": "2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "postcss-cli": "^6.0.0",
    "prismjs": "^1.15.0",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "rxjs": "^6.2.2",
    "ts-helpers": "^1.1.1",
    "ts-node": "~2.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~4.5.0",
    "typescript": "~2.9.2",
    "zone.js": "^0.8.16"
  },
  "dependencies": {
    "rxjs-compat": "^6.0.0-rc.0"
  }
}
